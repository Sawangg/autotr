    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/bun": "^1.1.12",
    "@types/elliptic": "^6.4.18",
    "@types/prompts": "^2.4.9"
  },
  "peerDependencies": {
    "typescript": "^5.6.3"
  }
